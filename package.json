{
  "name": "ErrorManager",
  "version": "1.0.0",
  "description": "Error middleware for compelex error handling",
  "main": "index.js",
  "scripts": {
    "unit-test": "mocha 'test/unit/**/*.test.js' --recursive --timeout 30000 --exit --parallel --reporter mochawesome --reporter-options reportDir=./website/report",
    "integration-test": "mocha 'test/integration/**/*.test.js' --recursive --timeout 30000 --exit --parallel --reporter mochawesome --reporter-options reportDir=./website/report",
    "test": "npm run integration-test && npm run unit-test"
  },
  "nodemonConfig": {
    "ignore": [
      "mochawesome-report/*"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/CyberT33N/errormanager.git"
  },
  "keywords": [
    "error",
    "middleware"
  ],
  "author": "CyberT33N",
  "license": "MIT",
  "bugs": {
    "url": "https://gitlab.com/CyberT33N/errormanager/issues"
  },
  "homepage": "https://gitlab.com/CyberT33N/errormanager#readme",
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "chai": "^4.3.4",
    "eslint": "^7.32.0",
    "eslint-config-google": "^0.14.0",
    "mocha": "^9.1.2",
    "nodemon": "^2.0.13",
    "nyc": "^15.1.0"
  }
}
