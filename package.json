{
  "name": "error-manager-helper",
  "version": "3.5",
  "description": "Error manager for complex error handling with and without express",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npx eslint src && tsup",
    "_kill": "kill -9 $(lsof -t -i:3876) || true",
    "test-only": "bash test-only.sh",
    "test": "vitest --coverage --disable-console-intercept --watch=false",
    "test:watch": "vitest --watch",
    "test:integration": "vitest run --testTimeout=300000 --coverage --disable-console-intercept --watch=false --config vitest.integration.config.ts",
    "test:unit": "vitest run --testTimeout=300000 --coverage --disable-console-intercept --watch=false --config vitest.unit.config.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CyberT33N/ErrorManager.git"
  },
  "keywords": [
    "error",
    "middleware",
    "manager"
  ],
  "author": "CyberT33N",
  "license": "MIT",
  "bugs": {
    "url": "https://gitlab.com/CyberT33N/errormanager/issues"
  },
  "homepage": "https://gitlab.com/CyberT33N/errormanager#readme",
  "devDependencies": {
    "@eslint/js": "^9.10.0",
    "@types/eslint__js": "^8.42.3",
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.11",
    "@types/sinon": "^17.0.3",
    "@vitest/coverage-v8": "^2.0.5",
    "axios": "^1.6.8",
    "eslint": "^9.10.0",
    "express": "^4.19.2",
    "express-async-errors": "^3.1.1",
    "glob": "^10.3.15",
    "globals": "^15.2.0",
    "sinon": "^18.0.0",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.2",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.4.0",
    "vitest": "^2.0.5"
  },
  "engines": {
    "node": ">=20"
  }
}
